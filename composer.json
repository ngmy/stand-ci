{
    "name": "ngmy/stand-ci",
    "description": "A continuous integration tool designed for Laravel 4 application",
    "keywords": [
        "laravel 4",
        "continuous integration",
        "ci",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ngmy",
            "email": "y.nagamiya@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.1",
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "1.*",
        "halleck45/phpmetrics": "1.*",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd" : "1.4.*",
        "pdepend/pdepend" : "1.1.1",
        "phpdocumentor/phpdocumentor": "2.*",
        "mayflower/php-codebrowser": "1.1.1",
        "nesbot/Carbon": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/commands"
        ],
        "psr-0": {
            "Ngmy\\StandCi\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
